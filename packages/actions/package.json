{
  "name": "actions",
  "version": "0.0.0",
  "license": "MIT",
  "sideEffects": false,
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "keywords": [
    "nextjs",
    "react",
    "zod",
    "server actions"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "dependencies": {
    "zod": "^3.21.4",
    "zod-validation-error": "^1.3.1"
  },
  "peerDependencies": {
    "next": ">= 13.4.0",
    "react": ">= 18.2.0",
    "zod": ">= 3.0.0"
  },
  "devDependencies": {
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "tsup": "^7.1.0",
    "typescript": "5.1.6"
  },
  "tsup": {
    "outDir": "dist",
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": true
  }
}
